    Response.Cache.SetCacheability(HttpCacheability.NoCache);
    Response.ContentEncoding = System.Text.Encoding.UTF8;
    Response.Cache.SetExpires(DateTime.Now.AddDays(10));
    Response.Write("CACHE MANIFEST");
}
